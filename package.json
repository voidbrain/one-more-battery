{
  "name": "angular-21-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docker:dev": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up --build -d",
    "gh-pages": "ng build --configuration production && angular-cli-ghpages --dir=dist/angular-21-project"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.0.1",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@capacitor/barcode-scanner": "^2.2.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.4.4",
    "@huggingface/transformers": "^3.8.0",
    "@ionic/angular": "^8.7.10",
    "@jsverse/transloco": "^8.2.0",
    "@xenova/transformers": "^2.17.2",
    "@zxing/library": "^0.21.3",
    "angular-cli-ghpages": "^2.0.3",
    "capacitor": "^0.5.6",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.39.1",
    "ngx-device-detector": "^10.1.0",
    "prettier": "^3.6.2",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "21.0.1",
    "@angular-eslint/eslint-plugin": "21.0.1",
    "@angular-eslint/eslint-plugin-template": "21.0.1",
    "@angular-eslint/schematics": "21.0.1",
    "@angular-eslint/template-parser": "21.0.1",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@angular/localize": "^21.0.0",
    "@capacitor/cli": "^7.4.4",
    "@types/jasmine": "~5.1.13",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "angular-eslint": "^21.0.1",
    "eslint": "9.39.1",
    "jasmine-core": "~5.12.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.3"
  }
}
